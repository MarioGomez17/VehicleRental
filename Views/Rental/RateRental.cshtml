@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Geologica:wght@100..900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/RateRental.css">
    <title>Calificar Alquiler</title>
</head>
<body class="BodyRateRental">
    <div class="ContentBodyRateRental">
        <div class="DivNavBarRateRental">
            <nav class="NavBarRateRental">
                <div class="LinkLogoRateRental">
                    <a asp-controller="Home" asp-action="Index"><img class="ImageLogo" src="~/images/Logo.webp" alt="Logotipo" /></a>
                </div>
                <div class="DivLinkRateRental">
                    <a asp-controller="User" asp-action="LogIn" class="LinkRateRental">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-login-2 IconLinkSVG"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#1f9102"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M9 8v-2a2 2 0 0 1 2 -2h7a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-7a2 2 0 0 1 -2 -2v-2" />
                            <path d="M3 12h13l-3 -3" />
                            <path d="M13 15l3 -3" />
                        </svg>
                        <span class="LinkTextRateRental">Iniciar Sesión</span>
                    </a>
                </div>
                <div class="DivLinkRateRental">
                    <a asp-controller="User" asp-action="UserInfo" class="LinkRateRental IconLinkSVG">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-user"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#020291"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M8 7a4 4 0 1 0 8 0a4 4 0 0 0 -8 0" />
                            <path d="M6 21v-2a4 4 0 0 1 4 -4h4a4 4 0 0 1 4 4v2" />
                        </svg>
                        <span class="LinkTextRateRental">Ver Mi Perfil</span>
                    </a>
                </div>
                <div class="DivLinkRateRental">
                    <a asp-controller="Vehicle" asp-action="VehiclesInfo" class="LinkRateRental IconLinkSVG">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-car"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#beac0d"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M7 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M17 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M5 17h-2v-6l2 -5h9l4 5h1a2 2 0 0 1 2 2v4h-2m-4 0h-6m-6 -6h15m-6 0v-5" />
                        </svg>
                        <span class="LinkTextRateRental">Ver Mis Vehiculos</span>
                    </a>
                </div>
                <div class="DivLinkRateRental">
                    <a asp-controller="Rental" asp-action="RentalHistory" class="LinkRateRental IconLinkSVG">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-history"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#000000"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M12 8l0 4l2 2" />
                            <path d="M3.05 11a9 9 0 1 1 .5 4m-.5 5v-5h5" />
                        </svg>
                        <span class="LinkTextRateRental">Ver Mis Alquileres</span>
                    </a>
                </div>
                <div class="DivLinkRateRental">
                    <a asp-controller="User" asp-action="LogOut" class="LinkRateRental">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-login IconLinkSVG"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#be0d0d"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M15 8v-2a2 2 0 0 0 -2 -2h-7a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h7a2 2 0 0 0 2 -2v-2" />
                            <path d="M21 12h-13l3 -3" />
                            <path d="M11 15l-3 -3" />
                        </svg>
                        <span class="LinkTextRateRental">Cerrar Sesión</span>
                    </a>
                </div>
            </nav>
        </div>
        <div class="DivContentRateRental">
            <div class="DivHeaderRateRental">
                <header class="HeaderRateRental">
                    <h1 class="MainTitlelRateRental">Alquiler de Vehículos</h1>
                </header>
            </div>
            <div class="DivMainContentRateRental">
                <div class="DivMainRateRental">
                    <main role="main" class="MainRateRental">
                        <div class="ContentMainRateRental">
                            <p>Tipo de Vehiculo</p>
                            <p>Clasificacion</p>
                            <p>Marca</p>
                            <p>Linea</p>
                            <p>Modelo</p>
                            <p>Cilindraje</p>
                            <p>Color</p>
                            <p>Ciudad</p>
                            <p>Fecha Inicio</p>
                            <p>Fecha Fin</p>
                            <form>
                                <label>puntucuacion</label>
                                <input type="text" name="name" value="" />
                                <label>Comentario</label>
                                <textarea></textarea>
                                <input type="submit" name="name" value="Calificar Alquiler" />
                            </form>
                        </div>
                    </main>
                </div>
            </div>
            <div class="DivFooterRateRental">
                <footer class="FooterRateRental">
                    <span class="TextFooterRateRental">
                        &copy; 2024 - Alquiler de Vehículos por Mario Gómez y Juan Diego Claros
                    </span>
                </footer>
            </div>
        </div>
    </div>
</body>
</html>
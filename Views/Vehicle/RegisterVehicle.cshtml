@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Geologica:wght@100..900&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="~/css/RegisterVehicle.css">
    <title>Registrar Vehículo</title>
</head>
<body class="BodyRegisterVehicle">
    <div class="ContentBodyRegisterVehicle">
        <div class="DivNavBarRegisterVehicle">
            <nav class="NavBarRegisterVehicle">
                <div class="LinkLogoRegisterVehicle">
                    <a asp-controller="Home" asp-action="Index"><img class="ImageLogo" src="~/images/Logo.webp" alt="Logotipo" /></a>
                </div>
                <div class="DivLinkRegisterVehicle">
                    <a asp-controller="User" asp-action="LogIn" class="LinkRegisterVehicle">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-login-2"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#00b341"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M9 8v-2a2 2 0 0 1 2 -2h7a2 2 0 0 1 2 2v12a2 2 0 0 1 -2 2h-7a2 2 0 0 1 -2 -2v-2" />
                            <path d="M3 12h13l-3 -3" />
                            <path d="M13 15l3 -3" />
                        </svg>
                        <span class="LinkTextRegisterVehicle">Iniciar Sesión</span>
                    </a>
                </div>
                <div class="DivLinkRegisterVehicle">
                    <a asp-controller="User" asp-action="UserInfo" class="LinkRegisterVehicle">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-user"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#00abfb"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M8 7a4 4 0 1 0 8 0a4 4 0 0 0 -8 0" />
                            <path d="M6 21v-2a4 4 0 0 1 4 -4h4a4 4 0 0 1 4 4v2" />
                        </svg>
                        <span class="LinkTextRegisterVehicle">Ver Mi Perfil</span>
                    </a>
                </div>
                <div class="DivLinkRegisterVehicle">
                    <a asp-controller="Vehicle" asp-action="VehiclesInfo" class="LinkRegisterVehicle">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-car"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#ffec00"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M7 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M17 17m-2 0a2 2 0 1 0 4 0a2 2 0 1 0 -4 0" />
                            <path d="M5 17h-2v-6l2 -5h9l4 5h1a2 2 0 0 1 2 2v4h-2m-4 0h-6m-6 -6h15m-6 0v-5" />
                        </svg>
                        <span class="LinkTextRegisterVehicle">Ver Mis Vehiculos</span>
                    </a>
                </div>
                <div class="DivLinkRegisterVehicle">
                    <a asp-controller="Rental" asp-action="RentalHistory" class="LinkRegisterVehicle">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-history"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#000000"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M12 8l0 4l2 2" />
                            <path d="M3.05 11a9 9 0 1 1 .5 4m-.5 5v-5h5" />
                        </svg>
                        <span class="LinkTextRegisterVehicle">Ver Mis Alquileres</span>
                    </a>
                </div>
                <div class="DivLinkRegisterVehicle">
                    <a asp-controller="User" asp-action="LogOut" class="LinkRegisterVehicle">
                        <svg xmlns="http://www.w3.org/2000/svg" class="icon icon-tabler icon-tabler-login"
                             width="35" height="35" viewBox="0 0 24 24" stroke-width="1.5" stroke="#ff2825"
                             fill="none" stroke-linecap="round" stroke-linejoin="round">
                            <path stroke="none" d="M0 0h24v24H0z" fill="none" />
                            <path d="M15 8v-2a2 2 0 0 0 -2 -2h-7a2 2 0 0 0 -2 2v12a2 2 0 0 0 2 2h7a2 2 0 0 0 2 -2v-2" />
                            <path d="M21 12h-13l3 -3" />
                            <path d="M11 15l-3 -3" />
                        </svg>
                        <span class="LinkTextRegisterVehicle">Cerrar Sesión</span>
                    </a>
                </div>
            </nav>
        </div>
        <div class="DivContentRegisterVehicle">
            <div class="DivHeaderRegisterVehicle">
                <header class="HeaderRegisterVehicle">
                    <h1 class="MainTitlelRegisterVehicle">Alquiler de Vehículos</h1>
                </header>
            </div>
            <div class="DivMainContentRegisterVehicle">
                <div class="DivMainRegisterVehicle">
                    <main role="main" class="MainRegisterVehicle">
                        <div class="ContentMainRegisterVehicle">
                            <form action="/" method="post">
                                <label>Placa</label>
                                <input type="text" name="name" value="" />
                                <label>Cilindrada</label>
                                <input type="text" name="name" value="" />
                                <label>Modelo</label>
                                <input type="text" name="name" value="" />
                                <label>Ciudad</label>
                                <select></select>
                                <label>Color</label>
                                <input type="text" name="name" value="" />
                                <label>Cantidad de Pasajeros</label>
                                <input type="text" name="name" value="" />
                                <label>Tipo de Vehículo</label>
                                <select></select>
                                <label>Clasificación del Vehículo</label>
                                <select></select>
                                <label>Tipo de Combustible</label>
                                <select></select>
                                <label>Marca</label>
                                <select></select>
                                <label>Linea</label>
                                <select></select>
                                <label>Número de Certificado CDA</label>
                                <input type="text" name="name" value="" />
                                <label>Número de Póliza de Seguro</label>
                                <input type="text" name="name" value="" />
                                <input type="submit" value="Registrar Vehículo" />
                            </form>
                        </div>
                    </main>
                </div>
            </div>
            <div class="DivFooterRegisterVehicle">
                <footer class="FooterRegisterVehicle">
                    <span class="TextFooterRegisterVehicle">
                        &copy; 2024 - Alquiler de Vehículos por Mario Gómez y Juan Diego Claros
                    </span>
                </footer>
            </div>
        </div>
    </div>
</body>
</html>